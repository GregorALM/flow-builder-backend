[gitflow "prefix"]
	bugfix = bugfix-
	feature = feature-
	release = release-
	hotfix = hotfix-
	support = support-
	versiontag = v
[gitflow "branch"]
	master = master
	develop = develop
[gitflow "feature.finish"]
	push = true
	no-ff = true
[gitflow "bugfix.finish"]
	push = true
	no-ff = true
[gitflow "hotfix.finish"]
	push = true
	no-ff = true
[gitflow "release.finish"]
	push = true
	no-ff = true
